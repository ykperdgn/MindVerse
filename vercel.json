{
  "version": 2,
  "name": "mindverse",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "has": [
        { "host": "mindversedaily.com" }
      ],
      "status": 308,
      "headers": { "Location": "https://www.mindversedaily.com/$1" },
      "continue": false
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/404.html",
      "status": 404
    }
  ]
}
